<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace表示命名空间 -->
<mapper namespace="com.zsc.flower.dao.ReviewDao">
    <select id="selectRevieByPid" resultType="com.zsc.flower.domain.entity.Review" parameterType="Long">
        select * from review where pid=#{pid}
    </select>
</mapper>